\n.container {\n  margin-right: auto;\n  margin-left: auto;\n}\n\n.text-center {\n  text-align: center;\n}\n\n.btn {\n  display: inline-block;\n  font-weight: 400;\n  text-align: center;\n  white-space: nowrap;\n  vertical-align: middle;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n  border: 1px solid transparent;\n  padding: 0.375rem 0.75rem;\n  font-size: 1rem;\n  line-height: 1.5;\n  border-radius: 0.25rem;\n  transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out, border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;\n}\n\n.btn:focus, .btn:hover {\n  text-decoration: none;\n}\n\n.btn:focus, .btn.focus {\n  outline: 0;\n  box-shadow: 0 0 0 0.2rem rgba(0, 123, 255, 0.25);\n}\n\n.btn.disabled, .btn:disabled {\n  opacity: .65;\n}\n\n.btn:not([disabled]):not(.disabled):active, .btn:not([disabled]):not(.disabled).active {\n  background-image: none;\n}\n\n.btn-secondary {\n  background-color: #868e96;\n  border-color: #868e96;\n}\n\n.btn-secondary:hover {\n  border-color: #6c757d;\n}\n\n.btn-secondary:focus, .btn-secondary.focus {\n  box-shadow: 0 0 0 0.2rem rgba(134, 142, 150, 0.5);\n}\n\n.btn-secondary.disabled, .btn-secondary:disabled {\n  background-color: #868e96;\n  border-color: #868e96;\n}\n\n.btn-secondary:not([disabled]):not(.disabled):active, .btn-secondary:not([disabled]):not(.disabled).active,\n.show > .btn-secondary.dropdown-toggle {\n  border-color: #666e76;\n  box-shadow: 0 0 0 0.2rem rgba(134, 142, 150, 0.5);\n}\n\n.container {\n  max-width: 100vmin;\n}\n\n.grid-container {\n  padding: 0;\n  margin-top: 16px;\n}\n\n.square {\n  position: relative;\n  display: inline-block;\n  border-right: 1px solid rgba(0, 0, 0, 0.2);\n  border-bottom: 1px solid rgba(0, 0, 0, 0.2);\n}\n\n.square-content {\n  position: absolute;\n  height: 100%;\n  width: 100%;\n  line-height: 1.5;\n  cursor: default;\n  color: rgba(0, 0, 0, 0.7);\n  font-size: 7vmin;\n  z-index: 9999;\n}\n\n.focused-content {\n  background-color: #FFF59D !important;\n  box-shadow: inset 0 0 16px rgba(0, 0, 0, 0.4)\n}\n\n.highlight {\n  background-color: rgba(0, 0, 0, 0.1);\n}\n\n.clue {\n  color: #1976D2;\n}\n\n.square:nth-child(4n) {\n  border-left: none;\n}\n\n.square:first-child {\n  border-left: 3px solid rgba(0, 0, 0, 0.6);\n}\n\n.square:nth-child(3n) {\n  border-right: 3px solid rgba(0, 0, 0, 0.6);\n}\n\n.row:first-child > .square {\n  border-top: 3px solid rgba(0, 0, 0, 0.6);\n}\n\n.row:nth-child(3n) > .square {\n  border-bottom: 3px solid rgba(0, 0, 0, 0.6);\n}\n\n.row:first-child > .square:first-child {\n  border-top-left-radius: 3px;\n}\n\n.row:first-child > .square:last-child {\n  border-top-right-radius: 3px;\n}\n\n.row:last-child > .square:first-child {\n  border-bottom-left-radius: 3px;\n}\n\n.row:last-child > .square:last-child {\n  border-bottom-right-radius: 3px;\n}\n\n.row:nth-child(4n) > .square {\n  border-top: none;\n}\n\n.numbers-container,\n.edit-container {\n  padding: 0;\n}\n\n.edit-container {\n  margin-bottom: 24px;\n}\n\n.btn {\n  font-family: monospace;\n  margin-top: 8px;\n  color: rgba(0, 0, 0, 0.6);\n  background: transparent;\n}\n\n.number-btn {\n  width: 9.5vmin;\n  padding: 0;\n  font-size: 9vmin;\n}\n\n.edit-button {\n  width: 24vmin;\n  font-size: 5vmin;\n  padding-top: 4px;\n  padding-bottom: 4px;\n}"},"lineCount":216}},"error":null,"hash":"245a6402662027a7f6c1e69ae4c3194f","cacheData":{"env":{}}}